{
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools",
    "debug.allowBreakpointsEverywhere": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.defaultFormatter": "matklad.rust-analyzer",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.guides.bracketPairs": "active",
    "editor.inlineSuggest.enabled": true,
    "files.associations": {
        "*.cppm": "cpp",
        "*.ixx": "cpp",
        "array": "cpp",
        "string_view": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "bitset": "cpp",
        "deque": "cpp",
        "initializer_list": "cpp",
        "iterator": "cpp",
        "queue": "cpp",
        "random": "cpp",
        "stack": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "utility": "cpp",
        "vector": "cpp",
        "*.tcc": "cpp",
        "algorithm": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "xfacet": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocinfo": "cpp",
        "xlocnum": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp",
        "format": "cpp",
        "charconv": "cpp",
        "chrono": "cpp",
        "forward_list": "cpp",
        "functional": "cpp",
        "iomanip": "cpp",
        "list": "cpp",
        "locale": "cpp",
        "map": "cpp",
        "mutex": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "stop_token": "cpp",
        "thread": "cpp",
        "xhash": "cpp",
        "xlocbuf": "cpp",
        "xlocmes": "cpp",
        "xlocmon": "cpp",
        "xloctime": "cpp",
        "xtree": "cpp",
        "cinttypes": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cwctype": "cpp",
        "set": "cpp",
        "future": "cpp",
        "numbers": "cpp",
        "numeric": "cpp",
        "regex": "cpp",
        "semaphore": "cpp",
        "variant": "cpp",
        "span": "c",
        "*.ipp": "cpp"
    },
    "files.watcherExclude": {
        "**/*.db-*": true,
        "**/*.prof*": true,
        "**/.git/**": true,
        "**/coverage/**": true,
        "**/documentation/**": true,
        "**/profile/**": true,
        "**/release/**": true,
        "**/target/**": true,
        "**/test/**": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": true,
        "yaml": true
    },
    "lldb.verboseLogging": true,
    "redhat.telemetry.enabled": true,
    "rust-analyzer.cargo.allFeatures": false,
    "rust-analyzer.checkOnSave.command": "clippy",
    //"rust-analyzer.checkOnSave.overrideCommand": ["make", "clippy", "MESSAGE_FORMAT=json"],
    "terminal.integrated.sendKeybindingsToShell": true,
    "window.zoomLevel": 1,
    "workbench.iconTheme": "material-icon-theme",
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    }
}