{
    "C_Cpp.default.configurationProvider": "vector-of-bool.cmake-tools",
    "debug.allowBreakpointsEverywhere": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.defaultFormatter": "matklad.rust-analyzer",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "files.associations": {
        "array": "cpp",
        "string_view": "cpp",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "bitset": "cpp",
        "deque": "cpp",
        "initializer_list": "cpp",
        "iterator": "cpp",
        "queue": "cpp",
        "random": "cpp",
        "stack": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "utility": "cpp",
        "vector": "cpp",
        "*.tcc": "cpp",
        "algorithm": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "ios": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "xfacet": "cpp",
        "xiosbase": "cpp",
        "xlocale": "cpp",
        "xlocinfo": "cpp",
        "xlocnum": "cpp",
        "xmemory": "cpp",
        "xstddef": "cpp",
        "xstring": "cpp",
        "xtr1common": "cpp",
        "xutility": "cpp"
    },
    "files.watcherExclude": {
        "**/*.db-*": true,
        "**/*.prof*": true,
        "**/.git/**": true,
        "**/coverage/**": true,
        "**/documentation/**": true,
        "**/release/**": true,
        "**/target/**": true,
        "**/test/**": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": false,
        "plaintext": true,
        "yaml": true
    },
    "lldb.verboseLogging": true,
    "redhat.telemetry.enabled": true,
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.checkOnSave.overrideCommand": ["make", "clippy", "CLIPPY_MESSAGE_FORMAT=json"],
    "sqltools.useNodeRuntime": true,
    "terminal.integrated.sendKeybindingsToShell": true,
    "workbench.iconTheme": "material-icon-theme"
}
